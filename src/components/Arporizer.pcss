.root {
  table {
    border: 2px solid #000000;
  }
}

.td {
  border: 2px solid #000000;
  width: 60px;
  text-align: center;
  vertical-align: middle;
}

.selected {
  background-color: rgba(0, 0, 0, 0.5);
  color: #ffffff;
}

.lastSelected {
  background-color: rgba(0, 0, 0, 1);
  color: #ffffff;
}
